{
	"name": "GW TC Builder",
	"version": "2.6",
	"description": "SAP GW Test Case Builder",
	"devtools_page": "devtoolsBackground.html",
	"manifest_version": 2,
	"background": {
        "page":"background_page.html"
    },
	
	
	"web_accessible_resources": ["*"],
	"permissions": [
		"tabs",
		"clipboardWrite",
		"http://*/*",
		"https://*/*"
		],
	"sidebar_action": {
		"default_icon": {
			"16": "img/icon16.png",
			"48": "img/icon48.png"
		}
	},
		

	"content_security_policy": "script-src 'self'  'unsafe-eval'; object-src 'self'" ,
	
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	}
}